{
  "name": "SEGaurd",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_title": "SEGaurd"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'",
    "sandbox": "sandbox allow-scripts; script-src 'self' 'wasm-unsafe-eval'; worker-src 'self' blob:; object-src 'self'"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "storage",
    "unlimitedStorage",
    "downloads",
    "contextMenus"
  ],
  "host_permissions": ["<all_urls>"],
  "web_accessible_resources": [
    {
      "resources": ["models/*", "libs/*", "onnx_worker.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "sandbox": {
    "pages": ["sandbox.html"]
  }
}
