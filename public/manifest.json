{
    "name": "SEGaurd",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
      "default_popup": "popup.html",
      "default_title": "SEGaurd"
    },
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    },
    "permissions": ["storage"],
    "host_permissions": ["*://*/*"],
    "web_accessible_resources": [
      {
        "resources": ["models/*"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  